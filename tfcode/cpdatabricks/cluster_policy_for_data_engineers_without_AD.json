{
  "cluster_type": {
    "type": "whitelist",
    "values": [
      "all-purpose"
    ]
  },
  "cluster_name": {
    "type": "unlimited",
    "defaultValue": "login_DataEngineer_policy"
  },
  "instance_pool_id": {
    "type": "unlimited",
    "isOptional": false
  },
  "num_workers": {
    "type": "range",
    "minValue": 0,
    "maxValue": 10,
    "defaultValue": 1
  },
  "spark_version": {
    "type": "regex",
    "pattern": "6\\.[4]+\\.x-scala.*|7\\.[0-9]+\\.x-scala.*|8\\.[0-9]+\\.x-scala.*"
  },
  "enable_local_disk_encryption": {
    "type": "fixed",
    "hidden": true,
    "value": true
  },
  "autotermination_minutes": {
    "type": "range",
    "minValue": 15,
    "maxValue": 120,
    "defaultValue": 30
  },
  "spark_conf.spark.databricks.delta.preview.enabled": {
    "type": "fixed",
    "value": "true"
  },
  "custom_tags.team": {
    "type": "unlimited",
    "isOptional": false
  },
  "custom_tags.cost_center": {
    "type": "unlimited",
    "isOptional": false
  }
}